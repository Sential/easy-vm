{
  "name": "easy-vm",
  "version": "1.2.1",
  "description": "Run securely untrusted code with whitelisted Node modules",
  "main": "build/index",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "eslint --ext .ts --ext .js src/",
    "lint-fix": "prettier-eslint --write src/**/*.ts",
    "prepare": "npm run build",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sentialx/easy-vm.git"
  },
  "keywords": [
    "vm",
    "nodejs"
  ],
  "author": "Sential",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sentialx/easy-vm/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/sentialx/easy-vm#readme",
  "devDependencies": {
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "prettier": "1.16.4"
  },
  "dependencies": {
    "@types/node": "10.12.24",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3"
  }
}
