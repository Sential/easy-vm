{
  "name": "easy-vm",
  "version": "1.2.0",
  "description": "Run securely untrusted code with whitelisted Node modules",
  "main": "build/index",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "eslint --ext .ts --ext .js src/",
    "lint-fix": "prettier-eslint --write src/**/*.ts",
    "prepare": "npm run build",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sential/easy-vm.git"
  },
  "keywords": [
    "vm",
    "nodejs"
  ],
  "author": "Sential",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sential/easy-vm/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Sential/easy-vm#readme",
  "devDependencies": {
    "eslint": "5.2.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "prettier": "1.13.7",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "typescript-eslint-parser": "16.0.1"
  },
  "dependencies": {
    "@types/node": "10.5.3",
    "ts-loader": "4.4.2",
    "typescript": "2.9.2"
  }
}
