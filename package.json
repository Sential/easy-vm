{
  "name": "easy-vm",
  "version": "1.1.0",
  "description": "Run securely untrusted code with whitelisted Node modules",
  "main": "build/index",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "webpack-cli --mode development --watch",
    "build": "webpack-cli --mode production",
    "lint": "eslint --ext .ts --ext .js src/",
    "lint-fix": "prettier-eslint --write src/**/*.ts",
    "prepare": "npm run build",
    "test": "npm run build && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sential/easy-vm.git"
  },
  "keywords": ["vm", "nodejs"],
  "author": "Sential",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sential/easy-vm/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Sential/easy-vm#readme",
  "devDependencies": {
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "prettier": "1.11.1",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "typescript-eslint-parser": "14.0.0"
  },
  "dependencies": {
    "@types/node": "9.6.0",
    "ts-loader": "4.1.0",
    "typescript": "2.7.2",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  }
}
